<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".InsertActivity">

    <EditText
            android:layout_width="219dp"
            android:layout_height="63dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editAddress"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.755" app:layout_constraintVertical_bias="0.413"/>
    <EditText
            android:layout_width="219dp"
            android:layout_height="63dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editTel"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.755" app:layout_constraintVertical_bias="0.512"/>
    <EditText
            android:layout_width="219dp"
            android:layout_height="63dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editEmail"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.755" app:layout_constraintVertical_bias="0.608"/>
    <TextView
            android:text="주소"
            android:textSize="20dp"
            android:gravity="center"
            android:layout_width="126dp"
            android:layout_height="65dp" android:id="@+id/textView4"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.204" app:layout_constraintVertical_bias="0.414"/>
    <TextView
            android:text="전화번호"
            android:textSize="20dp"
            android:gravity="center"
            android:layout_width="126dp"
            android:layout_height="65dp" android:id="@+id/textView5"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.204" app:layout_constraintVertical_bias="0.513"/>
    <TextView
            android:text="이메일"
            android:textSize="20dp"
            android:gravity="center"
            android:layout_width="126dp"
            android:layout_height="65dp" android:id="@+id/textView6"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.204" app:layout_constraintVertical_bias="0.61"/>
    <TextView
            android:text="추가"
            android:textSize="40dp"
            android:gravity="center"
            android:layout_width="238dp"
            android:layout_height="108dp" android:id="@+id/textView2"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.12"/>
    <TextView
            android:text="이름"
            android:textSize="20dp"
            android:gravity="center"
            android:layout_width="126dp"
            android:layout_height="65dp" android:id="@+id/textView3"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.204" app:layout_constraintVertical_bias="0.317"/>
    <EditText
            android:layout_width="219dp"
            android:layout_height="63dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editName"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.755" app:layout_constraintVertical_bias="0.319"/>
    <Button
            android:text="추가하기"
            android:layout_width="177dp"
            android:layout_height="65dp" android:id="@+id/insertBtn"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.527" app:layout_constraintVertical_bias="0.801"/>
</androidx.constraintlayout.widget.ConstraintLayout>